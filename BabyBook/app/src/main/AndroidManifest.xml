<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="babybook.com.v1" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logobabysteps"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Profile"
            android:label="@string/app_name"
            android:noHistory = "true">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:label="HomePage" >


        </activity>
        <activity
            android:name=".DisplayStoryList"
            android:label="DisplayStory" >


        </activity>
        <activity
            android:name=".WritePage"
            android:label="@string/title_activity_write_page" >
        </activity>

        <activity
            android:name=".ReadStory"
            android:label="@string/title_activity_read_story" >
        </activity>
    </application>

</manifest>
